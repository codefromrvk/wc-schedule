---
import BaseLayout from "@/layouts/BaseLayout.astro";
import Hero from "@/components/landing/Hero.astro";
import Intro from "@/components/landing/Intro.astro";
import { AstroSeo } from "@astrolib/seo";
import Schedule from "@/components/landing/Schedule.astro";
---

<AstroSeo
  title="ICC ODI Worldcup Schedule"
  description="ICC ODI Worldcup schedule India. List of all matches (IND vs PAK,ENG vs AUS) "
  canonical="https://worldcup.rvkay.in"
  openGraph={{
    url: "https://worldcup.rvkay.in",
    title: "Worldcup Schedule",
    description: "ICC ODI Worldcup schedule India. List of all matches (IND vs PAK,ENG vs AUS)",
    images: [
      {
        url: "https://worldcup.rvkay.in",
        width: 1200,
        height: 630,
        alt: "Social open graph",
        type: "image/jpeg",
      },
      {
        url: "https://worldcup.rvkay.in",
        width: 4096,
        height: 4096,
        alt: "Twitter open graph",
        type: "image/jpeg",
      },
      { url: "https://worldcup.rvkay.in" },
      { url: "https://worldcup.rvkay.in" },
    ],
    site_name: "YourSitesName",
  }}
  twitter={{
    handle: "@0xrvk",
    site: "@site",
    cardType: "summary_large_image",
  }}
/>
<BaseLayout>
  <Hero />
  <Schedule/>
  <Intro />
</BaseLayout>
