---
import BaseLayout from "@/layouts/BaseLayout.astro";
import Hero from "@/components/landing/Hero.astro";
import Intro from "@/components/landing/Intro.astro";
import { AstroSeo } from "@astrolib/seo";
import Schedule from "@/components/landing/Schedule.astro";
---

<AstroSeo
  title="2023 ICC ODI Worldcup Schedule"
  description="2023 ICC ODI Worldcup schedule India. List of all matches (IND vs PAK,ENG vs AUS) "
  canonical="https://worldcup.rvkay.in"
  openGraph={{
    url: "https://worldcup.rvkay.in",
    title: "2023 ICC ODI Worldcup Schedule",
    description:
      "ICC ODI Worldcup schedule India. List of all matches (IND vs PAK,ENG vs AUS)",
    images: [
      {
        url: "https://worldcup.rvkay.in",
        width: 1200,
        height: 630,
        alt: "Social open graph",
        type: "image/jpeg",
      },
      {
        url: "https://worldcup.rvkay.in",
        width: 4096,
        height: 4096,
        alt: "Twitter open graph",
        type: "image/jpeg",
      },
      { url: "https://worldcup.rvkay.in" },
      { url: "https://worldcup.rvkay.in" },
    ],
    site_name: "ODI Worldcup schedule website",
  }}
  twitter={{
    handle: "@0xrvk",
    site: "@site",
    cardType: "Worldcup schedule",
  }}
/>
<BaseLayout>
  <link
    href="
https://cdn.jsdelivr.net/npm/hint.css@2.7.0/hint.min.css
"
    rel="stylesheet"
  />
  <Hero />
  <Schedule />
  <Intro />
</BaseLayout>
